    \end{equation}

snippet mds
abbr    Mathds environment
    \mathds{${0}}

snippet mbf
abbr    Mathbf environment
    \mathbf{${0}}

snippet tbf
abbr    Bold Text environment
    \textbf{${0}}

snippet multicolumn
abbr    Multi-column table cell
    \multicolumn{${1:2}}{${2:c}}{${0}}

snippet sfig
abbr    Subfigure environment
options head
    \begin{subfigure}[${1:htbp}]{${2:\\textwidth}}
        \centering
        \includegraphics[${3:width=\\textwidth}]{${4:path to image}}
        \caption{${5:caption text}}\label{sfig:${6:label text}}
    \end{subfigure}

snippet itemizeit
abbr    Itemize environment with leftmargin and textit
options head
    \begin{itemize}[leftmargin=${1:4em}]
        \item \textit{${2}}: ${3}
    \end{itemize}

snippet itemit
abbr    Item with textit
options head
    \item \textit{${1}}: ${2}

snippet itembf
abbr    Item with textbf
options head
    \item \textbf{${1}}: ${2}

snippet comment
abbr    Comment environment
options head
    \begin{comment}
        ${0}
    \end{comment}
